<% if @favorite_books.any? %>
  <% @favorite_books.each do |favorite_book| %>
    <li>
      <%= link_to restaurant_path(favorite_book) do %>
        <div class="aaa">
          <%= book_image_select(favorite_book)%>
          <div class="clearfix"></div>
        </div>
        <strong><%= favorite_book.title %></strong>
      <% end %>
  <hr>
    </li>
  <% end %>
<% end %>

